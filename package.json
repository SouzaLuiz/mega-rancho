{
  "name": "mega-rancho",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "npm install && node -r esm src/index.js",
    "nodemon": "nodemon -r esm src/server.js",
    "browser-sync": "browser-sync start --proxy 'localhost:3333' --files 'public/**, src/app/view/**'",
    "dev:sass": "sass --watch --style=compressed ./src/sass/index.scss:./src/public/css/styles.css",
    "dev": "npm-run-all -p nodemon dev:sass browser-sync"
  },
  "license": "MIT",
  "dependencies": {
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "mongoose": "^5.9.19",
    "multer": "^1.4.2",
    "node-telegram-bot-api": "^0.50.0",
    "nunjucks": "^3.2.1",
    "sharp": "^0.25.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.29.0"
  }
}
