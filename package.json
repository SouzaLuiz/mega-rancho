{
  "name": "mega-rancho",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "npm install && build:sass && node src/index.js",
    "nodemon": "nodemon src/server.js",
    "browser-sync": "browser-sync start --proxy 'localhost:3333' --files 'public/**, src/view/**'",
    "dev:sass": "sass --watch --style=compressed ./src/sass/index.scss:public/css/styles.css",
    "dev": "npm-run-all -p nodemon dev:sass browser-sync"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.19",
    "multer": "^1.4.2",
    "node-telegram-bot-api": "^0.50.0",
    "nunjucks": "^3.2.1",
    "sharp": "^0.25.4"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.29.0"
  }
}
